<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BlurLayout">
        <attr name="downscaleFactor" format="float" />
        <attr name="blurRadius" format="integer" />
        <attr name="fps" format="integer" />
        <attr name="overlaySurface" format="boolean" />
    </declare-styleable>
    <declare-styleable name="ParticipantView">
        <attr name="state" format="enum">
            <enum name="video" value="0"/>
            <enum name="no_video" value="1"/>
            <enum name="selected" value="2"/>
        </attr>
        <attr name="identity" format="reference"/>
        <attr name="mirror" format="boolean" />
        <attr name="type" format="enum">
            <enum name="fit" value="0"/>
            <enum name="fill" value="1"/>
            <enum name="balanced" value="2"/>
        </attr>

    </declare-styleable>
    <declare-styleable name="ClearableEditText">
        <attr name="clearIcon" format="reference" />
    </declare-styleable>
</resources>